<!DOCTYPE html>
<div id="{{C_o.DIV_ID_s}}_stat_{{C_o.STAT_a[2]}}">
    <input id="{{C_o.INPUT_ID_s}}_{{C_o.STAT_a[2]}}_canvas" name="{{C_o.STAT_a[2]}}_nav" type="radio" checked>
    <input id="{{C_o.INPUT_ID_s}}_{{C_o.STAT_a[2]}}_settings" name="{{C_o.STAT_a[2]}}_nav" type="radio">

    <div id="{{C_o.DIV_ID_s}}_{{C_o.STAT_a[2]}}_settings">
        <aside>
            <nav>
                <label for="{{C_o.INPUT_ID_s}}_{{C_o.STAT_a[2]}}_canvas" data-legend="{{C_o.NAV_LEGEND_o.stat_close.legend_s}}">
                    <span data-role="open">{{C_o.NAV_LEGEND_o.empty}}</span>
                </label>
            </nav>

            <ul id="{{C_o.LIST_ID_s}}_{{C_o.STAT_a[2]}}_layer_items">
            </ul>

            <div id="{{C_o.DIV_ID_s}}_{{C_o.STAT_a[2]}}_layer_setup">
                <label id="{{C_o.LABEL_ID_s}}_{{C_o.STAT_a[2]}}_layer_add">{{C_o.NAV_LEGEND_o.layers_add.legend_s}}</label>
                <label id="{{C_o.LABEL_ID_s}}_{{C_o.STAT_a[2]}}_layer_hide">{{C_o.NAV_LEGEND_o.layers_hide.legend_s}}</label>
            </div>

            <ul id="{{C_o.LIST_ID_s}}_{{C_o.STAT_a[2]}}_layer_setop">
              <li>
                <input  id="{{C_o.INPUT_ID_s}}_{{C_o.STAT_a[2]}}_layer_set_none" type="radio" name=layer_set checked/>
                <label for="{{C_o.INPUT_ID_s}}_{{C_o.STAT_a[2]}}_layer_set_none">{{C_o.NAV_LEGEND_o.layers_none.legend_s}}</label>
              <li>
                <input  id="{{C_o.INPUT_ID_s}}_{{C_o.STAT_a[2]}}_layer_set_union" type="radio" name=layer_set />
                <label for="{{C_o.INPUT_ID_s}}_{{C_o.STAT_a[2]}}_layer_set_union">{{C_o.NAV_LEGEND_o.layers_union.legend_s}}</label>
              <li>
                <input  id="{{C_o.INPUT_ID_s}}_{{C_o.STAT_a[2]}}_layer_set_difference" type="radio" name=layer_set />
                <label for="{{C_o.INPUT_ID_s}}_{{C_o.STAT_a[2]}}_layer_set_difference">{{C_o.NAV_LEGEND_o.layers_difference.legend_s}}</label>
              <li>
                <input  id="{{C_o.INPUT_ID_s}}_{{C_o.STAT_a[2]}}_layer_set_intersection" type="radio" name=layer_set />
                <label for="{{C_o.INPUT_ID_s}}_{{C_o.STAT_a[2]}}_layer_set_intersection">{{C_o.NAV_LEGEND_o.layers_intersection.legend_s}}</label>
              <li>
                <input  id="{{C_o.INPUT_ID_s}}_{{C_o.STAT_a[2]}}_layer_set_complement" type="radio" name=layer_set />
                <label for="{{C_o.INPUT_ID_s}}_{{C_o.STAT_a[2]}}_layer_set_complement">{{C_o.NAV_LEGEND_o.layers_complement.legend_s}}</label>
              <li id="{{C_o.LIST_ITEM_ID_s}}_{{C_o.STAT_a[2]}}_layer_set_deviation">
                <label>{{C_o.NAV_LEGEND_o.layers_deviation.legend_s}}</label>
                <input  id="{{C_o.INPUT_ID_s}}_{{C_o.STAT_a[2]}}_layer_set_deviation" type="range" min="0" max="100" step="1" value="{{C_o.PAINT_DEVIATION_n}}" data-tip="{{C_o.PAINT_DEVIATION_n}}"/>
            </ul>

            <div id="{{C_o.DIV_ID_s}}_{{C_o.STAT_a[2]}}_slide_hue">
                <input id="{{C_o.INPUT_ID_s}}_{{C_o.STAT_a[2]}}_hue_rangeX_n" type="range" min="1" max="100" value="50" data-tip="50"/>
                <input id="{{C_o.INPUT_ID_s}}_{{C_o.STAT_a[2]}}_hue_rangeY_n" type="range" min="1" max="360" value="60" data-tip="60"/>
                <div>
                  <canvas id="{{C_o.CANVAS_ID_s}}_{{C_o.STAT_a[2]}}_hue_back" width=100 height=720></canvas>
                  <canvas id="{{C_o.CANVAS_ID_s}}_{{C_o.STAT_a[2]}}_hue_front" width=100 height=720></canvas>
                </div>
                <label id="{{C_o.LABEL_ID_s}}_{{C_o.STAT_a[2]}}_hue_trace">
                    <span data-icon="{{C_o.NAV_LEGEND_o.empty}}">{{C_o.NAV_LEGEND_o.empty}}</span>
                    <output id="output_{{C_o.STAT_a[2]}}_hue" type="text">{{S_o.hue_back}}</output>
                    <!--<output id="output_{{C_o.STAT_a[2]}}_hue_rate" type="text">0,001</output>-->
                </label>
            </div>

            <div id="{{C_o.DIV_ID_s}}_{{C_o.STAT_a[2]}}_slide_sat">
                <input id="{{C_o.INPUT_ID_s}}_{{C_o.STAT_a[2]}}_sat_rangeX_n" type="range" min="1" max="100" value="50" data-tip="50"/>
                <input id="{{C_o.INPUT_ID_s}}_{{C_o.STAT_a[2]}}_sat_rangeY_n" type="range" min="1" max="100" value="10" data-tip="10"/>
                <div>
                  <canvas id="{{C_o.CANVAS_ID_s}}_{{C_o.STAT_a[2]}}_sat_back" width=100 height=200></canvas>
                  <canvas id="{{C_o.CANVAS_ID_s}}_{{C_o.STAT_a[2]}}_sat_front" width=100 height=200></canvas>
                </div>
                <label id="{{C_o.LABEL_ID_s}}_{{C_o.STAT_a[2]}}_sat_trace">
                    <span data-icon="{{C_o.NAV_LEGEND_o.empty}}">{{C_o.NAV_LEGEND_o.empty}}</span>
                    <output id="output_{{C_o.STAT_a[2]}}_sat" type="text">{{S_o.back_sat}}</output>
                    <!--<output id="output_{{C_o.STAT_a[2]}}_sat_rate" type="text">0,001</output>-->
                </label>
            </div>

            <div id="{{C_o.DIV_ID_s}}_{{C_o.STAT_a[2]}}_slide_lum">
                <input id="{{C_o.INPUT_ID_s}}_{{C_o.STAT_a[2]}}_lum_rangeX_n" type="range" min="1" max="100" value="50" data-tip="50"/>
                <input id="{{C_o.INPUT_ID_s}}_{{C_o.STAT_a[2]}}_lum_rangeY_n" type="range" min="1" max="100" value="10" data-tip="10"/>
                <div>
                  <canvas id="{{C_o.CANVAS_ID_s}}_{{C_o.STAT_a[2]}}_lum_back" width=100 height=200></canvas>
                  <canvas id="{{C_o.CANVAS_ID_s}}_{{C_o.STAT_a[2]}}_lum_front" width=100 height=200></canvas>
                </div>
                <label id="{{C_o.LABEL_ID_s}}_{{C_o.STAT_a[2]}}_lum_trace">
                    <span data-icon="{{C_o.NAV_LEGEND_o.empty}}">{{C_o.NAV_LEGEND_o.empty}}</span>
                    <output id="output_{{C_o.STAT_a[2]}}_lum" type="text">{{S_o.back_lum}}</output>
                    <!--<output id="output_{{C_o.STAT_a[2]}}_lum_rate" type="text">0,001</output>-->
                </label>
            </div>
        </aside>
    </div>

    <!--<div data-layer_n=0></div>-->

    <div id="{{C_o.DIV_ID_s}}_{{C_o.STAT_a[2]}}_layer_view">

        <label for="{{C_o.INPUT_ID_s}}_{{C_o.STAT_a[2]}}_settings" data-legend="{{C_o.NAV_LEGEND_o.stat_open.legend_s}}">
            <span data-role="open">{{C_o.NAV_LEGEND_o.empty}}</span>
        </label>

        <input id="{{C_o.INPUT_ID_s}}_{{C_o.STAT_a[2]}}_view_perspective" type="range" min="100" max="5000" step="100" value="{{C_o.PAINT_PERSPECTIVE_n}}" data-tip="{{C_o.PAINT_PERSPECTIVE_n}}"/>
        <label for="{{C_o.INPUT_ID_s}}_{{C_o.STAT_a[2]}}_view_perspective">{{C_o.NAV_LEGEND_o.layers_perspective.legend_s}}</label>
        <input id="{{C_o.INPUT_ID_s}}_{{C_o.STAT_a[2]}}_view_distance" type="range" min="-1000" max="1000" step="50" value="{{C_o.PAINT_DISTANCE_n}}" data-tip="{{C_o.PAINT_DISTANCE_n}}"/>
        <label for="{{C_o.INPUT_ID_s}}_{{C_o.STAT_a[2]}}_view_distance">{{C_o.NAV_LEGEND_o.layers_distance.legend_s}}</label>
        <input id="{{C_o.INPUT_ID_s}}_{{C_o.STAT_a[2]}}_view_rotate" type="range" min="-200" max="200" step="10" value="{{C_o.PAINT_ROTATE_n}}" data-tip="{{C_o.PAINT_ROTATE_n}}"/>
        <label for="{{C_o.INPUT_ID_s}}_{{C_o.STAT_a[2]}}_view_rotate">{{C_o.NAV_LEGEND_o.layers_rotate.legend_s}}</label>
        <input id="{{C_o.INPUT_ID_s}}_{{C_o.STAT_a[2]}}_view_scale" type="range" min=".1" max="1" step=".05" value="{{C_o.PAINT_SCALE_n}}" data-tip="{{C_o.PAINT_SCALE_n}}"/>
        <label for="{{C_o.INPUT_ID_s}}_{{C_o.STAT_a[2]}}_view_scale">{{C_o.NAV_LEGEND_o.layers_scale.legend_s}}</label>
    </div>

    <script id="script_{{C_o.STAT_a[2]}}" data-src="/assets/scripts/js/{{C_o.STAT_a[2]}}.min.js"></script>
</div>
