<section id="{{C_o.SECTION_a[2]}}">

  <label id="{{C_o.LABEL_ID_s}}_{{C_o.SECTION_a[2]}}_zoom_out">{{C_o.ZOOM_OUT_s}}</label>
  <!--<input id="{{C_o.INPUT_ID_s}}_{{C_o.SECTION_a[2]}}_zoom_out" type="checkbox">-->

  <label for="{{C_o.INPUT_ID_s}}_{{C_o.SECTION_a[2]}}_selectAll">
    <span>[</span>
    <span>]</span>
  </label>
  <input id="{{C_o.INPUT_ID_s}}_{{C_o.SECTION_a[2]}}_selectAll" type="checkbox" checked />

  <div id="{{C_o.DIV_ID_s}}_{{C_o.SECTION_a[2]}}_masonry">
  </div>

  <div id="{{C_o.DIV_ID_s}}_{{C_o.SECTION_a[2]}}_dock">
    <nav>
      <label for="{{C_o.DIV_ID_s}}_{{C_o.SECTION_a[2]}}_dock_nav" data-legend="{{C_o.NAV_LEGEND_o.stat_shadow_unfold.legend_s}}">
          <span data-role="open">{{C_o.NAV_LEGEND_o.empty}}</span>
      </label>
    </nav>
    <input id="{{C_o.DIV_ID_s}}_{{C_o.SECTION_a[2]}}_dock_nav" type="checkbox">
    <div>
      <label for="{{C_o.INPUT_ID_s}}_{{C_o.SECTION_a[2]}}_wrap_n">{{C_o.NAV_LEGEND_o.expo_wrap.legend_s}}</label>
      <input id="{{C_o.INPUT_ID_s}}_{{C_o.SECTION_a[2]}}_wrap_n" type="range" min="1" max="10" step="1" value="{{C_o.EXPO_WRAP_n}}" data-tip="{{C_o.EXPO_WRAP_n}}"/>
  
      <label for="{{C_o.DIV_ID_s}}_{{C_o.SECTION_a[2]}}_dock_nav" id="{{C_o.LABEL_ID_s}}_{{C_o.SECTION_a[2]}}_hide">{{C_o.NAV_LEGEND_o.expo_hide.legend_s}}</label>
      <label for="{{C_o.DIV_ID_s}}_{{C_o.SECTION_a[2]}}_dock_nav" id="{{C_o.LABEL_ID_s}}_{{C_o.SECTION_a[2]}}_remove">{{C_o.NAV_LEGEND_o.expo_remove.legend_s}}</label>
      <label for="{{C_o.DIV_ID_s}}_{{C_o.SECTION_a[2]}}_dock_nav" id="{{C_o.LABEL_ID_s}}_{{C_o.SECTION_a[2]}}_group">{{C_o.NAV_LEGEND_o.expo_group.legend_s}}</label>
      <label for="{{C_o.DIV_ID_s}}_{{C_o.SECTION_a[2]}}_dock_nav" id="{{C_o.LABEL_ID_s}}_{{C_o.SECTION_a[2]}}_local">{{C_o.NAV_LEGEND_o.expo_add.legend_s}}</label>
      <label for="{{C_o.DIV_ID_s}}_{{C_o.SECTION_a[2]}}_dock_nav" id="{{C_o.LABEL_ID_s}}_{{C_o.SECTION_a[2]}}_caption">{{C_o.NAV_LEGEND_o.expo_caption.legend_s}}</label>
      <label for="{{C_o.DIV_ID_s}}_{{C_o.SECTION_a[2]}}_dock_nav" id="{{C_o.LABEL_ID_s}}_{{C_o.SECTION_a[2]}}_save">{{C_o.NAV_LEGEND_o.expo_save.legend_s}}</label>
      <label for="{{C_o.DIV_ID_s}}_{{C_o.SECTION_a[2]}}_dock_nav" id="{{C_o.LABEL_ID_s}}_{{C_o.SECTION_a[2]}}_restore">{{C_o.NAV_LEGEND_o.expo_restore.legend_s}}</label>
      <label for="{{C_o.DIV_ID_s}}_{{C_o.SECTION_a[2]}}_dock_nav" id="{{C_o.LABEL_ID_s}}_{{C_o.SECTION_a[2]}}_burst">{{C_o.NAV_LEGEND_o.burst.legend_s}}</label>
    </div>

  </div>

  <dialog id="dialog_{{C_o.SECTION_a[2]}}_caption">
    <form id="form_{{C_o.SECTION_a[2]}}_caption" method="dialog">
      <div id="{{C_o.DIV_ID_s}}_{{C_o.SECTION_a[2]}}_caption">
        <header>
          <label control>cartel</label>
          <label for="{{C_o.INPUT_ID_s}}_{{C_o.SECTION_a[2]}}_caption_cancel">&#10006;</label>
          <input id="{{C_o.INPUT_ID_s}}_{{C_o.SECTION_a[2]}}_caption_cancel" type="checkbox">
          <label for="{{C_o.INPUT_ID_s}}_{{C_o.SECTION_a[2]}}_caption_reset">&#8634;</label>
          <input id="{{C_o.INPUT_ID_s}}_{{C_o.SECTION_a[2]}}_caption_reset" type="checkbox">
          <label for="{{C_o.INPUT_ID_s}}_{{C_o.SECTION_a[2]}}_caption_accept">&#10003;</label>
          <input id="{{C_o.INPUT_ID_s}}_{{C_o.SECTION_a[2]}}_caption_accept" type="checkbox">
        </header>
        <ul id="{{C_o.LIST_ID_s}}_{{C_o.SECTION_a[2]}}_caption_list">
          <li id=item_1>
            <label control>créateur</label>
            <input id="{{C_o.INPUT_ID_s}}_{{C_o.SECTION_a[2]}}_caption_artist_s" type="text">
          <li id=item_2>
            <label control>sujet</label>
            <input id="{{C_o.INPUT_ID_s}}_{{C_o.SECTION_a[2]}}_caption_subject_s" type="text">
          <li id=item_3>
            <label control>date de création</label>
            <input id="{{C_o.INPUT_ID_s}}_{{C_o.SECTION_a[2]}}_caption_year_n" type="text">
          <li id=item_4>
            <label control>hauteur</label>
            <input id="{{C_o.INPUT_ID_s}}_{{C_o.SECTION_a[2]}}_caption_w_height_n" type="text">
          <li id=item_5>
            <label control>largeur</label>
            <input id="{{C_o.INPUT_ID_s}}_{{C_o.SECTION_a[2]}}_caption_w_width_n" type="text">
          <li id=item_6>
            <label control>collection</label>
            <input id="{{C_o.INPUT_ID_s}}_{{C_o.SECTION_a[2]}}_caption_location_s" type="text">
          <li id=item_7>
            <label control>lieu</label>
            <input id="{{C_o.INPUT_ID_s}}_{{C_o.SECTION_a[2]}}_caption_place_s" type="text">
          <li id=item_8>
            <label control>pays</label>
            <input id="{{C_o.INPUT_ID_s}}_{{C_o.SECTION_a[2]}}_caption_country_s" type="text">
          <li id=item_9>
            <label control>lien</label>
            <input id="{{C_o.INPUT_ID_s}}_{{C_o.SECTION_a[2]}}_caption_link_s" type="text">
        </ul>
      </div>
    </form>
  </dialog>

</section>
