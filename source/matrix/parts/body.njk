{% set _work_s = '' %}
{% set _wh_s = '' %}
{% set _stat_s = '' %}
{% if workID_s %}
{% set _work_s = ' data-work_s=' + workID_s %}
{% set _wh_s = ' data-wh_s=' +  widthHeight_s %}
{% endif %}
{% if stat_s %}
{% set _stat_s = ' data-stat_s=' + stat_s %}
{% endif %}
{% if layer_a %}
{% set _layer_s = ' data-layer_s="' + layer_a.join( ' ' ) + '"' %}
{% endif %}

<body{{_work_s}}{{_wh_s}}{{_stat_s}}{{_layer_s}}>
<a id="{{C_o.SCREEN_TOP}}"></a>
{% include "parts/body/nav_input.njk" %}
{% include "parts/body/main.njk" %}

{% if stat_s %}
{% include "parts/body/iframe_stat.njk" %}
{% endif %}

{% if stat_s === 'burst' %}
<label id="goto_{{C_o.MAIN_DOCK}}" for="{{C_o.INPUT_ID_s}}_dock_nav">{{C_o.MAIN_DOCK_s}}</label>
<input id="{{C_o.INPUT_ID_s}}_{{C_o.STAT_a[0]}}_equal" type="checkbox">
<div id="goto_{{C_o.RADIAL_EQUAL}}"></div>
<label id="goto_{{C_o.SCREEN_CENTER}}" for="{{C_o.INPUT_ID_s}}_{{C_o.STAT_a[0]}}_hue_img_position" tabindex=1>{{C_o.GOTO_CENTER_s}}</label>
<label id="goto_{{C_o.SCREEN_FULL}}">{{C_o.SCREEN_FULL_s}}</label>
{% endif %}

{% if ( section_a.length > 1 ) and ( section_a[2] === 'expo' ) %}
<label id="goto_{{C_o.MAIN_DOCK}}" for="{{C_o.INPUT_ID_s}}_dock_nav">{{C_o.MAIN_DOCK_s}}</label>
<label id="goto_{{C_o.SCREEN_FULL}}">{{C_o.SCREEN_FULL_s}}</label>
{% endif %}
{% if reserve_s %}
<label id="goto_{{C_o.SCREEN_FULL}}">{{C_o.SCREEN_FULL_s}}</label>
{% endif %}

<label id="goto_{{C_o.SCREEN_TOP}}">{{C_o.GOTO_TOP_s}}</label>
{% include "parts/body/nav_label.njk" %}    <!-- at end to be upper layer -->
{#% include "parts/body/svg_inline.njk" %#}
{% include "parts/body/script.njk" %}
</body>
